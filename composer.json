{
    "name": "mvs/api-oauth2-zend3",
    "description": " Integrando uma API Rest Full com Oauth2 e Zend Framework 3s",
    "version": "1.0.0",
    "type": "project",
    "license": "MIT",
    "keywords": ["framework", "mvc", "zf", "api"],
    "require": {
        "php": "^7.2",
        "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-servicemanager-di": "^1.0",
        "mongodb/mongodb": "^1.1",
        "bshaffer/oauth2-server-php": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Connection\\": "module/Connection/src/",
            "Authorization\\": "module/Authorization/src"
        }
    }
}
